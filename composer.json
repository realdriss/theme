{
    "name": "realdriss/theme",
    "description": "Theme package",
    "type": "package",
    "autoload": {
        "psr-4": {
            "RealDriss\\Theme\\": "src"
        }
    },
    "require": {
        "realdriss/platform": "*@dev",
        "realdriss/sitemap": "*@dev",
        "realdriss/widget": "*@dev",
        "realdriss/slug": "*@dev",
        "realdriss/seo-helper": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RealDriss\\Theme\\Providers\\ThemeServiceProvider",
                "RealDriss\\Theme\\Providers\\RouteServiceProvider"
            ],
            "aliases": {
                "Theme": "RealDriss\\Theme\\Facades\\ThemeFacade",
                "ThemeOption": "RealDriss\\Theme\\Facades\\ThemeOptionFacade",
                "ThemeManager": "RealDriss\\Theme\\Facades\\ManagerFacade",
                "AdminBar": "RealDriss\\Theme\\Facades\\AdminBarFacade",
                "SiteMapManager": "RealDriss\\Theme\\Facades\\SiteMapManagerFacade"
            }
        }
    }
}
